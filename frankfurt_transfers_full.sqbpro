SELECT 
    Player, Age, Position, Nationality, 
    &quot;From Club&quot;, &quot;To Club&quot;, &quot;Fee (EUR)&quot;, Season
FROM transfers
WHERE &quot;Fee (EUR)&quot; IS NOT NULL
ORDER BY &quot;Fee (EUR)&quot; DESC
LIMIT 15;</sql><sql name="SQL 2">--Ortalama transfer yaşı 

SELECT AVG(Age) AS avg_age
FROM transfers
WHERE Age IS NOT NULL;</sql><sql name="SQL 4">--En çok satış geliri elde edilen sezonlar

SELECT 
    Season, 
    SUM(&quot;Fee (EUR)&quot;) AS total_income
FROM transfers
WHERE Direction = 'Departure'
GROUP BY Season
ORDER BY Season;</sql><sql name="SQL 5">--Milliyetlere göre en çok transfer edilen oyuncular

SELECT 
    Nationality, 
    COUNT(*) AS player_count
FROM transfers
GROUP BY Nationality
ORDER BY player_count DESC
LIMIT 10;</sql><sql name="SQL 6">--Pozisyona göre ortalama transfer ücreti

SELECT 
    Position, 
    AVG(&quot;Fee (EUR)&quot;) AS avg_fee
FROM transfers
WHERE &quot;Fee (EUR)&quot; IS NOT NULL
GROUP BY Position
ORDER BY avg_fee DESC;</sql><sql name="SQL 7">--Gelen gidenlerin yaş ortalaması

SELECT 
    Direction,
    AVG(Age) AS avg_age
FROM transfers
WHERE Age IS NOT NULL
GROUP BY Direction;</sql><sql name="SQL 8">--Hangi pozisyondan kaç adet transfer edilmiş. 

SELECT 
    Position,
    SUM(CASE WHEN Direction = 'Arrival' THEN 1 ELSE 0 END) AS arrivals_count,
    SUM(CASE WHEN Direction = 'Departure' THEN 1 ELSE 0 END) AS departures_count
FROM transfers
GROUP BY Position
ORDER BY arrivals_count DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
